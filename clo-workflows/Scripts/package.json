    "core-decorators": "^0.20.0",
    "mobx": "^3.3.2",
    "mobx-react": "^4.3.2",
    "mobx-react-devtools": "^4.2.15",
    "office-ui-fabric-react": "^5.42.0",
    "react": "16.2",
    "react-dom": "^16.1.1",
    "react-router": "^4.2.0",
