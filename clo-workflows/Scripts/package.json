{
  "name": "clo-workflows",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev:local": "webpack-cli --env.NODE_ENV=local --mode=development --watch",
    "dev:sharepoint": "webpack-cli --watch  --env.NODE_ENV=sharepoint",
    "dev:sharepointProxy": "concurrently --kill-others \"webpack-cli --watch --env.NODE_ENV=sharepointProxy --mode=development\" \"npm run sp:proxyServer \"",
    "build:local": "webpack-cli --env.NODE_ENV=local",
    "build:sharepoint": "webpack-cli --env.NODE_ENV=sharepoint",
    "build:sharepointProxy": "webpack-cli --env.NODE_ENV=sharepointProxy",
    "build:production": "webpack-cli --env.NODE_ENV=sharepoint --mode production",
    "lint": "ts-node ./scripts/lint.ts",
    "test": "rimraf test_build/** && tsc --outDir test_build && cpx  \"./res/**/*.json\" \"./test_build/res\" && ava test_build/test --verbose",
    "sp:deleteAll": "ts-node ./sp_config/migrations/tasks/Init && ts-node ./sp_config/migrations/tasks/DeleteAll",
    "sp:createAll": "ts-node ./sp_config/migrations/tasks/Init && ts-node ./sp_config/migrations/tasks/CreateAll &&  ts-node ./sp_config/migrations/tasks/CreateGroups",
    "sp:deleteAllAndCreateAll": "ts-node ./sp_config/migrations/tasks/Init && ts-node ./sp_config/migrations/tasks/DeleteAll && ts-node ./sp_config/migrations/tasks/CreateAll",
    "sp:createMissingGroups": "ts-node ./sp_config/migrations/tasks/Init && ts-node ./sp_config/migrations/tasks/CreateGroups",
    "sp:proxyServer": "node ./sp_config/proxy-server/server.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/core-decorators": "^0.10.32",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "@types/shelljs": "^0.7.9",
    "ava": "^0.24.0",
    "chalk": "^2.3.1",
    "concurrently": "^3.5.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.5",
    "mini-css-extract-plugin": "^0.4.0",
    "react-docgen-typescript": "^1.2.6",
    "react-styleguidist": "^7.0.4",
    "shelljs": "^0.8.2",
    "source-map-loader": "^0.2.1",
    "sp-pnp-node": "^2.0.2",
    "sp-rest-proxy": "^2.5.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-loader": "^4.2.0",
    "ts-mockito": "^2.2.7",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "2.7",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "@pnp/common": "^1.0.4",
    "@pnp/logging": "^1.0.4",
    "@pnp/nodejs": "^1.0.4",
    "@pnp/odata": "^1.0.4",
    "@pnp/sp": "^1.0.4",
    "@pnp/sp-addinhelpers": "^1.0.4",
    "@uifabric/icons": "^5.4.0",
    "core-decorators": "^0.20.0",
    "mobx": "^3.3.2",
    "mobx-react": "^4.3.2",
    "mobx-react-devtools": "^4.2.15",
    "office-ui-fabric-react": "^5.79.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "sp-pnp-js": "^3.0.4"
  },
  "husky": {
    "hooks": {
      "pre-push": ["npm run test", "npm run lint"]
    }
  }
}
