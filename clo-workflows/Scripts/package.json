    "@types/react-router-dom": "^4.0.8",
    "ava": "^0.24.0",
    "chalk": "^2.3.0",
    "source-map-loader": "^0.2.1",
    "sp-pnp-node": "^2.0.2",
    "ts-loader": "^3.1.1",
    "ts-mockito": "^2.2.7",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
