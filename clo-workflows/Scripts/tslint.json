{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "double"],
        "semicolon": [true, "never"],
        "ordered-imports": false,
        "arrow-parens": false,
        "space-before-function-paren": false,
        "whitespace": false,
        "member-ordering": false,
        "max-line-length": [true, 160],
        "array-type": false,
        "member-access": false,
        "forin": false
    },
    "rulesDirectory": []
}
